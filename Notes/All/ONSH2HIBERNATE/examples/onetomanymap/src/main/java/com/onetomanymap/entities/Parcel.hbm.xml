<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="com.onetomanymap.entities">
	<class name="Parcel" table="parcel">
		<id name="parcelNo" column="parcel_no">
			<generator class="increment" />
		</id>
		<property name="description" />
		<property name="weight" />
		<property name="source" />
		<property name="destination" />
		<property name="parcelDate" column="parcel_dt" />
		<property name="shippingAmount" column="shipping_amt" />
	</class>
</hibernate-mapping>