<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping
	package="com.onetomanylist.entities">
	<class name="Token" table="token">
		<id name="tokenNo" column="token_no">
			<generator class="increment" />
		</id>
		<property name="serviceType" column="service_type" />
		<property name="problem" column="problem" />
		<property name="tokenIssuedDate" column="token_issued_dt" />
		<property name="customerName" column="customer_nm" />
		<property name="mobileNo" column="mobile_no" />
	</class>
</hibernate-mapping>