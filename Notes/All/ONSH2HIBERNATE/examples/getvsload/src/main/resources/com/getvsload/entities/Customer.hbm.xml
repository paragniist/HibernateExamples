<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="com.getvsload.entities">
	<class name="Customer" table="customer" proxy="ICustomer">
		<id name="customerNo" column="customer_no" />
		<property name="customerName" column="customer_nm" />
		<property name="dob" />
		<property name="gender" />
		<property name="mobileNo" column="mobile_no" />
		<property name="emailAddress" column="email_address" />
	</class>
</hibernate-mapping>